<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<PackageOutputPath>$(SolutionDir)Packages/</PackageOutputPath>
		<TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
		<DisableImplicitFrameworkReferences Condition="'$(TargetFramework)'=='net462'">true</DisableImplicitFrameworkReferences>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<Authors>chenhongjin</Authors>
		<Company>chenhongjin</Company>
		<Copyright>438865652@qq.com，chenhongjin</Copyright>
		<Version>1.3.24.64-piero</Version>
		<PackageProjectUrl>https://www.gtkapp.com</PackageProjectUrl>
		<PackageReleaseNotes>
			C# interface cross-platform development component gtksystem-windows-forms
		</PackageReleaseNotes>
		<Title>GTK cross-platform winform interface component, compiled once, can run on windows, linux, macos platforms</Title>
		<Description>
			It is a cross-platform interface component developed based on GTK components and compatible with the native C# control winform interface.
			Use this component GTKSystem.Drawing to compile once and run across platforms windows\linux\macos.
			Update platform:
			https://www.gtkapp.com；
			https://gitee.com/easywebfactory/gtksystem-windows-forms；
			https://github.com/easywebfactory/gtksystem-windows-forms；
		</Description>
		<WarningsAsErrors>CS8618;CS8600</WarningsAsErrors>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageLicenseExpression>LGPL-3.0-only</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/easywebfactory/gtksystem-windows-forms.git</RepositoryUrl>
		<PackAsTool>False</PackAsTool>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<LangVersion>latest</LangVersion>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\..\GtkForms.snk</AssemblyOriginatorKeyFile>
		<NoWarn>$(NoWarn);NUnit2015;NUnit2017</NoWarn>
		<Nullable>enable</Nullable>
		<RootNamespace>System.Drawing</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<IsAotCompatible>True</IsAotCompatible>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	</PropertyGroup>
	<ItemGroup>
		<Compile Include="..\GTKSystem.Windows.Forms\GTKControls\Drawing\**\*.cs" Link="%(RecursiveDir)%(Filename)%(Extension)" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\README.en.md" Link="README.md" Pack="true" PackagePath="\README.md" />
		<None Include="..\..\README.md" Link="README.zh-HANS.md" Pack="true" PackagePath="\README.zh-HANS.md" />
		<None Include="..\licenses\LICENSE.txt" Pack="True" PackagePath="\" />
	</ItemGroup>
	<ItemGroup>
		<InternalsVisibleTo Include="Gtk.Windows.Forms.Tests, " />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="System" Condition="'$(TargetFramework)'=='net462'" />
		<Reference Include="System.Core" Condition="'$(TargetFramework)'=='net462'" />
		<Reference Include="System.Linq" Condition="'$(TargetFramework)'=='net462'" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="GtkSharp" Version="3.24.24.95" />
		<PackageReference Include="System.Memory" Version="4.6.1" />
		<PackageReference Include="System.Numerics.Vectors" Version="4.6.0" />
	</ItemGroup>
</Project>